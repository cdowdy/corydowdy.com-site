<!doctype html>
<html class="no-js" lang="{{ htmllang() }}">
<head>
    {% include 'webfonts/webfont.js.twig' %}
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta name="msapplication-TileImage" content="/favicon-144.png"/>
    <meta name="msapplication-TileColor" content="#ecf0f1"/>
    <meta name="msvalidate.01" content="346403273F73350B6928A46B28684A5B" />
    <meta name="mobile-web-app-capable" content="yes">
    <link rel="dns-prefetch" href="//cdn.corydowdy.com"/>
    <link rel="dns-prefetch" href="//use.typekit.net"/>
    <link rel="dns-prefetch" href="//www.google-analytics.com" />
    {# make sure we always display a proper title: The record's title if there is one, appended with the
       sitename. If there is no title, we append the sitename with the payoff, if there is one. #}
    <title>
        {% if record.title is defined %}{{ record.title|striptags }} | {% endif %}
        {{ app.config.get('general/sitename') }}
        {% if record.title is not defined and app.config.get('general/payoff') %} | {{ app.config.get('general/payoff') }}{% endif %}
    </title>
    {% include 'headers/_touchicons.twig' %}    {# paths.theme is always a correct path to the current theme. Even if you rename the theme-folder,
       this will keep working. See the documentation on paths: http://docs.bolt.cm/internals#paths #}
    <link rel="stylesheet" href="{{ paths.theme }}css/app.min.css">
    <style>
    .wf-loading h1{text-align:center;font-size:2.75rem;line-height:1.4;font-weight:700;color:#2C3E50;font-family:sans-serif;margin:.2rem 0 .5rem;}.wf-loading .magellan_nav li a{text-decoration:none;color:#999;font-family:sans-serif;padding:.1875rem 1rem;}.wf-loading h1 small{font-size:50%;color:#7F8C8D;line-height:0;font-weight:400;font-family:sans-serif;}.wf-loading h5{font-weight:700;color:#2C3E50;font-size:1.125rem;font-family:sans-serif;margin:.2rem 0 .5rem;}.wf-loading body p{font-weight:400;font-size:1rem;line-height:1.6;color:#444;margin:0 0 1.25rem;}
    </style>
</head>
<body>

<div class="off-canvas-wrap">
    <div class="inner-wrap">
        <div class="hide--small">
            <nav id="bt-menu" class="bt-menu">
                <a href="#" class="bt-menu-trigger"><span>Menu</span></a>
                {{ menu('main', '_menu_main.twig') }}
                <ul>
                    <li><a href="https://github.com/cdowdy" class="bt-icon icon-github">Code Hosted @ Github</a></li>
                </ul>
            </nav>
            <div class="row">
                <div class="small-10 columns">
                    <h1 class="header_logo"><a href="/"><img src="{{ paths.theme }}img/logo.svg" alt="Cory Dowdy Web Development logo"/> <small>//demos</small></a></h1>
                </div>
            </div>
        </div><!-- .hide - - small -->
        <div class="hide--large">
            <nav class="tab-bar">
                <section class="left-small">
                    <a class="left-off-canvas-toggle menu-icon" ><span></span></a>
                </section>
                <section class="middle tab-bar-section">
                    <h1 class="title"><a href="/">CoryDowdy</a></h1>
                </section>
            </nav>
        </div>


        <!-- Off Canvas Menu -->
        <aside class="left-off-canvas-menu">
            {{ menu('main', '_menu_offcanvas.twig') }}
        </aside>

        <section class="main-section">