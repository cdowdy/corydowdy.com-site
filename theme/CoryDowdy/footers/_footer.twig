<!-- Footer -->
<footer {% if record.slug == 'demo' %}id="sticky-footer"{% endif %}>
    <p class="text-center">&copy; Cory Dowdy Web Development</p>
</footer>

{% if record.slug == 'demo' %}
<script src="{{ paths.theme }}js/min/CoryDowdy.min.js"></script>
  <script>
    $(document).foundation();
  </script>
<script>
    $(window).bind("load", function () {
        var footer = $("#sticky-footer");
        var pos = footer.position();
        var height = $(window).height();
        height = height - pos.top;
        height = height - footer.height();
        if (height > 0) {
            footer.css({
                'margin-top': height + 'px'
            });
        }
    });
</script>
{% endif %}
