{% set dev = false %}
{% set cdn = pullzone1-corydowdywebdesi.netdna-ssl.com %}
<!doctype html>
<html class="no-js" lang="{{ htmllang() }}">
<head>
    <meta charset="utf-8" />
    {# Include DNS Prefetch  #}
    {#
       October 29 2014 move here because:
       https://github.com/h5bp/html5-boilerplate/blob/master/dist/doc/extend.md#explicit-prefetches
    #}
    {% block dns_prefetch %}
        <link rel="dns-prefetch" href="https://use.typekit.net"/>
        <link rel="dns-prefetch" href="https://p.typekit.net" />
        <link rel="dns-prefetch" href="https://www.google-analytics.com"/>
        {#<link rel="dns-prefetch" href="//pullzone1-corydowdywebdesi.netdna-ssl.com"/>#}
    {% endblock dns_prefetch %}

    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta name="theme-color" content="#2C3E50"/>
    <title>
        {% if record.title is defined %}{{ record.title|striptags }} | {% endif %}
        {{ app.config.get('general/sitename') }}
        {% if record.title is not defined and app.config.get('general/payoff') %} | {{ app.config.get('general/payoff') }}{% endif %}
    </title>
  {% block metaSeo %}
    <meta name="description" content="{{ record.metadescription }}" />
    {% if record.metakeywords %}
    <meta name="keywords" content="{{ record.metakeywords }}" />
  {% endif %}
  {% endblock metaSeo %}
    {# Bing Validation #}
    <meta name="msvalidate.01" content="346403273F73350B6928A46B28684A5B" />
    {# Chrome Webapp Capable #}
    <meta name="mobile-web-app-capable" content="yes">

    {% block touch_icons %}
        {% include 'touch_icons/_touch_icons.twig' %}
    {% endblock touch_icons  %}

    {# include javascripts / javascripts block #}
    {% block header_javascripts %}
      <script>
            {% spaceless %}
            {# Include TypeKit JavaScript #}
            {% include 'js/typekit/typekit.min.js' %}
            {% endspaceless %}
        </script>
        {# Modernizr:
            Current: 08-27-2014
            No Flexbox, No Flexbox Legacy, CSS Rem Unit, CSS Media Queryies
        #}
    {% endblock %}

    {% block enhance_js_meta %}
    <meta name="fullcss"  content="{{ paths.theme }}css/app04252015.min.css">
    <meta name="gpng" content="{{ paths.theme }}css/icons/icons.fallback.css">
    <meta name="gsvg" content="{{ paths.theme }}css/icons/icons.data.svg.css">
    <meta name="gdpng" content="{{ paths.theme }}css/icons/icons.data.png.css">
    {% endblock %}
  <!--[if lt IE 9]>
  <link rel="stylesheet" href="{{ paths.theme }}css/oldie.css">
  <![endif]-->
    {#  stylesheets block #}
    {% block stylesheets %}

        {% block fout %}
            {#
              Individual FOUT Styles Go in Page Template
              but they should probably be here because most
              carry over to each page.
              Gotta think of a better way
            #}
        {% endblock fout %}

        {% if app.request.cookies.has('fullcss') %}
    <link rel="stylesheet" href="{{ paths.theme }}css/app04252015.min.css"/>
    <link rel="stylesheet" href="{{ app.request.cookies.get('grunticon') }}" />
        {% else %}
            {% block inlined_styles %}
                {# Individual Inlined Styles Go in Page Template #}
            {% endblock %}

            {% block enhance %}
                <script>
                    {% spaceless %}
                    {# Include Enhance JS #}
                    {% include 'js/enhance/enhance-3-26-2015.min.js' %}
                    {% endspaceless %}
                </script>
            {% endblock enhance %}
        {% endif %}

            <noscript><link rel="stylesheet" href="{{ paths.theme }}css/app04252015.min.css"/></noscript>
            <noscript><link rel="stylesheet" href="{{ paths.theme }}css/icons/icons.fallback.css" ></noscript>

    {% endblock %}
    {% block picturefill %}
        {# dirty hack to include picturefill AFTER the noscript css#}
        <script src="{{ paths.theme }}js/lib/modernizr/modernizr.04242015.min.js" async ></script>
    {% endblock picturefill %}
</head>
<body>
{% block svgs %}
{% endblock svgs %}
<header class="fn-header" id="js-header" role="banner">
  <div class="header-wrap">
    <div class="fn-header__title-container">
      <h1 class="fn-header__name"><a href="/">Cory Dowdy</a></h1>
    </div>
    <input type="checkbox" id="fn-noJs" class="fn-navTrigger__no-js"/>
    <label id="fn-noJs_label" for="fn-noJs"></label>
    {# Insert Menu from menu directory #}
    {{ menu('main', 'menu/_menu.html.twig') }}
  </div>
</header>
<!-- /.fn-header -->

<main class="container fn-main" role="main">
    <!--[if lt IE 9]>
    <div class="ie_8_update_notice">
        <p class="browsehappy">
            You are using an <strong>outdated</strong> browser. Which is no longer supported.
            Please <a href="http://browsehappy.com/">upgrade your browser</a> to improve your experience and increase
            your security across the web.
        </p>
    </div>
    <![endif]-->
{# Main Body #}
{% block body %}

{% endblock body %}

{#  Footer block and closing main tag    #}

{% block footer %}
</main>





    {% block footer_javascripts %}
{% if dev == true %}
  {# development enviroment dont place GA in page #}
    {% else %}
      <script>
        (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
          (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
          m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
        })(window,document,'script','//www.google-analytics.com/analytics.js','ga');
        ga('create', 'UA-46388684-1', 'auto');
        ga('send', 'pageview');
      </script>
{% endif %}

    {% endblock %}

</body>
</html>
{% endblock %}
