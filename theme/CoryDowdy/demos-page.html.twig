{% extends 'base.html.twig' %}




{% block inlined_styles %}
    <style>
        {% include 'css/critical-css/demo-critical.16-10-2014.css' %}
    </style>
{% endblock %}


{% block body %}
    <div class="push-down">
        <div class="demo-link center-text">
            <figure>
                <a href="/demos/concrete5-magnific-popup"><span class="visuallyhidden">Concrete 5 Magnific Popup</span>
                    <svg class="center full-size-icon">
                        <use xlink:href="{{ paths.theme }}svgsprites/linkicons-sprite.svg#demo-c5-demo"></use>
                    </svg>
                </a>
                <figcaption>
                    <h4>
                        Concrete5 Magnific Popup
                    </h4>
                    <p>
                        Add-on using Magnific Popup Lightbox Plugin
                    </p>
                </figcaption>
            </figure>
        </div>
        <div class="demo-link center-text">
            <figure>
                <a href="/demos/concrete5-responsive-images"><span class="visuallyhidden">Concrete 5 Responsive Images</span>
                    <svg  class="center full-size-icon">
                        <use xlink:href="{{ paths.theme }}svgsprites/linkicons-sprite.svg#demo-c5-demo"></use>
                    </svg>
                </a>
                <figcaption>
                    <h4>
                        Concrete 5 Responsive Images
                    </h4>
                    <p>
                        Add-on using Picturefill and Foundation Interchange
                    </p>
                </figcaption>
            </figure>
        </div>
    </div>
    <!-- /.push-down -->
{% endblock %}

{% block footer_javascripts %}
    {{ parent() }}
    <script src="{{ paths.theme}}js/lib/off-canvas.min.js" async defer ></script>
{% endblock footer_javascripts %}
